    "@types/nodemailer": "^6.4.7",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "axios": "^1.3.4",
    "contentful-management": "^10.30.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "email-templates": "^11.0.3",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "firebase": "^9.15.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "i18next": "^22.1.4",
    "lodash": "^4.17.21",
    "marked": "^4.3.0",
    "next": "13.0.3",
    "next-i18next": "^13.0.0",
    "nodemailer": "^6.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.1.1",
    "react-lottie": "^1.2.3",
    "react-markdown": "^8.0.5",
    "react-spinners": "^0.13.8",
    "styled-components": "^5.3.6",
    "swiper": "^9.1.0",
