{
  "name": "ti-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "gen:microcopy": "tsc ./scripts/getTranslations.ts --outDir ./tempScripts && node ./tempScripts/scripts/getTranslations.js",
    "gen:blogsMetrics": "tsc ./scripts/updateBlogsMetrics.ts --outDir ./tempScripts && node ./tempScripts/scripts/updateBlogsMetrics.js",
    "gen:blogsList": "tsc ./scripts/getBlogs.ts --outDir ./tempScripts && node ./tempScripts/scripts/getBlogs.js",
    "prebuild": "npm run gen:microcopy && npm run gen:blogsList",
    "start": "next start",
    "eslint": "eslint",
    "nlint": "next lint",
    "lint": "eslint --ext .tsx,.ts .",
    "prettier": "prettier . --ignore-path .gitignore",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "type:check": "tsc",
    "validate": "run-p lint type:check format:check",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/font": "13.0.3",
    "@react-spring/web": "^9.6.1",
